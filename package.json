{
  "name": "fe-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "turbo run typecheck",
    "storybook:build": "turbo run build --filter=docs",
    "storybook:dev": "turbo run dev --filter=docs",
    "storybook:lint": "turbo run lint --filter=docs"
  },
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "eslint-plugin-prettier": "~5.5.4",
    "globals": "~16.5.0",
    "prettier": "~3.6.2",
    "turbo": "~2.5.8",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18"
  }
}
