{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "UI Core",
  "extends": "./base.json",
  "compilerOptions": {
    "importHelpers": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true
  }
}
